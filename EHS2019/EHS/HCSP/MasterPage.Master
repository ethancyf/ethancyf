                                    <asp:ImageButton ID="ibtnSSOConfirmOK" runat="server" AlternateText="<%$ Resources:AlternateText, OKBtn %>"
                                        ImageUrl="<%$ Resources:ImageUrl, OKBtn %>" OnClick="ibtnSSOConfirmOK_Click" /></td>
